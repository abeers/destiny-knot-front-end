<h1>{{model.identifier}}</h1>

{{pokemon-name-form submit="submit" reset="reset"}}

<h2>
  {{#each model.types as |type|}}
    {{type.identifier}}
  {{/each}}
</h2>

<select>
  {{#each model.abilities as |ability|}}
    <option value={{ability.id}}>{{ability.identifier}}</option>
  {{/each}}
</select>
